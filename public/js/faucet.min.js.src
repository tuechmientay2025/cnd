$.fn.country= function(options){
      return this.each(function(){
          var settings = $.extend({
            type:"address",
            init : function(a){ },
            callback : function(a){ },
            name:""
          },options,true);

          var modal = $(this).html(`<div class="boxcountry">
              
            ${settings.type=="address"?`<div class="form-group">
