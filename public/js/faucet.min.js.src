    background: url(data:image/svg+xml;base64,) no-repeat;
    z-index: 1000000000;
    background-position: center;
    background-size: contain;
    height: 45px;
    width: 45px;
    display: block;
    left: 46%;
    top: 45%;
}
.loaddata:after,[data-loader]:after{    
 content: " ";
    position: absolute;
     z-index: 100000000; 
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
    right: 0;
