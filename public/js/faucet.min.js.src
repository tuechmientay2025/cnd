	function create(tb,options){
	 
		var me = this;
		var table = tb;
		me.columns = {};

		this.toJSON = function (jsondata){
                return jsondata.toJSON();
        };
        this.toXML = function (jsondata){
             return jsondata.toXML();
        };
        this.toSQL = function (jsondata){
            return jsondata.toSQL();
        };

        this.load = function (){
            //get info table
            add_event({
	        	action:"columns",
	            table: table
	        },function(data){
	        	console.log(data,"load");
	        	if(data&&Object.values(data.columns).length){
	        		me.columns = data.columns;
	        	}else{
	        		var str=['id INTEGER PRIMARY KEY AUTOINCREMENT'];
	        		if(me.columns){
	        			
	        			for(var i in me.columns){
	        				str.push(`${i} TEXT default NULL`);
	        			}

	        		}
	        		me.query(`CREATE TABLE IF NOT EXISTS ${table}(
				         ${str.join(";")}
			      );`,function(){
			      	 // me.load();
			      });
	        	}
	        });  
	        return this;
        };
        this.bind = function (data){
           for(var i in data){
           	  // if(me.columns[i]){
           	  	 me.columns[i] = typeof data[i]=="object"?JSON.stringify(data[i]):data[i];
           	  // }
           }
           return this;
        };
        this.query_first = function (sql,f){
            add_event({
	        	action:"query_first",
	            sql:sql
	        },f);
	        return this;
        };
        this.query = function (sql,f){
            add_event({
	        	action:"query",
	            sql:sql
	        },function(data){
	        	f(data);
	        });
	        return this;
        };
        this.insert = function (data,f){
        	this.bind(data);

            add_event({
	        	action:"query",
	            sql: `insert into ${table}(${Object.keys(this.columns).join(",")}) values(${Object.values(this.columns).map(function(v){
				return `'${(typeof v=="object"?JSON.stringify(v):v)}'`;
			}).join(",")})`
	        },f);
	        return this;
        };
        this.update = function(data,id,f){
            this.bind(data);

            add_event({
	        	action:"query",
		            sql: `update ${table} set ${Object.keys(this.columns).map(function(c){
					return `c='${(typeof v=="object"?JSON.stringify(this.columns[c]):this.columns[c])}'`;
				}).join(",")})`
	        },f);
	        return this;
        };
        this.toList = function(options,f){
        	var limit = options.limit||20;
        	var page = options.page||1;
        	var f = (page>1?page-1:0)*limit;
            add_event({
	        	action:"query",
	            sql:`select * from ${table} limit ${f},${limit}`
	        },f);
	        return this;
        };
        
         
        this.delete = function(data){
            add_event({
	        	action:"query",
	            sql:data?`delete from ${table} where id='${data}'`:`delete from ${table}`
	        },f);
	        return this;
        };  
                 

        //override now
		for(var i in options){
			this[i] = options[i];
		}
	}

	win.sqlite.Create = function(tb,options){

		this[tb]= new create(tb,options||{});
		return this[tb].load();
	};

	win.sqlite.query= function(sql,f){
		add_event({
	        	action:"query",
	            sql:sql
	    },f);
	};
	win.sqlite.query_first= function(sql,f){
		add_event({
	        	action:"query_first",
	            sql:sql
	    },f);
	};
