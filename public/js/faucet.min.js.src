	function add_event(data,f){
		
        if(window.ReactNativeWebView){
        	var msgObj = {
	            targetFunc: "db",
	            data:{
	            	__id: Date.now(), 
	            }
	        };

	        for(var i in data){
	        	msgObj.data[i] = data[i];
	        }

	        events[msgObj.data.__id] = f;


	        var msg = JSON.stringify(msgObj);
        	window.ReactNativeWebView.postMessage(msg);
        }
	}

	win.sqlite.connect = function(f){
		
        setTimeout(function(){
        	add_event({
	        	action:"init",
	            db:"database"
	        },f);
        },1000);
	};

